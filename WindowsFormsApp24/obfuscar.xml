<?xml version='1.0'?>
<Obfuscator>
	<Var name="InPath" value="." />
	<Var name="OutPath" value=".\Obfuscator_Output" />
	
	<Var name="RegenerateDebugInfo" value="false" />
	<Var name="MarkedOnly" value="false" />
	<Var name="RenameProperties" value="true" />
	<Var name="RenameEvents" value="true" />
	<Var name="RenameFields" value="true" />
	<Var name="KeepPublicApi" value="false" />
	<Var name="HidePrivateApi" value="true" />
	<Var name="ReuseNames" value="false" />
	<Var name="HideStrings" value="true" />
	<Var name="OptimizeMethods" value="true" />
	<Var name="SuppressIldasm" value="true" />
	<Var name="AnalyzeXaml" value="true" />
	<Var name="UseUnicodeNames" value="false" />
	<Var name="UseKoreanNames" value="true" />

	<Module file="$(InPath)\WindowsFormsApp24.exe" />
	<Modules>
		<IncludeFiles>$(InPath)\ClassLibrary*.dll</IncludeFiles>
	</Modules>

	<AssemblySearchPath path="C:\Program Files (x86)\Reference Assemblies\Microsoft\Framework\.NETFramework\v4.8\" />
</Obfuscator>
